{
  "name": "phaser3-typescript-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Phaser3 Typescript Webpack Boilerplate",
  "scripts": {
    "build:web": "cross-env PLATFORM=web webpack --config webpack.prod.js",
    "build:android": "cross-env PLATFORM=android webpack --config webpack.prod.js",
    "build:ios": "cross-env PLATFORM=ios webpack --config webpack.prod.js",
    "dev": "cross-env PLATFORM=web webpack-dev-server --config webpack.dev.js"
  },
